	ansatz > spina_scapulae

muskelteil m_trapezius_pars_decendens {"name":"Musculus trapezius pars decendens"}
	is_a > m_trapezius
	funktion {"typ":"elevation"} > scapula
	funktion {"typ":"zieht_nach_kranial_medial"} > scapula

muskelteil m_trapezius_pars_transversa {"name":"Musculus trapezius pars transversa"}
	is_a > m_trapezius
	funktion {"typ":"zieht_nach_medial"} > scapula

muskelteil m_trapezius_aprs_ascendens {"name":"Musculus trapezius pars ascendens"}
	is_a > m_trapezius
	funktion {"typ":"zieht_nach_kaudalmedial"} > scapula

muskel m_rhombiodeus_minor {"name":"Musculus rhombiodeus minor"}
	is_a > schultermuskulatur
	innervation > n_dorsalis_scapulae
	ursprung > processus_spinosi_hwk_6_7
	ansatz > margo_medialis_scapula
	funktion {"typ":"fixation"} > scapula
	funktion {"typ":"zieht_nach_kranial_medial"} > scapula

muskel m_rhombiodeus_major {"name":"Musculus rhombiodeus major"}
	is_a > schultermuskulatur
	innervation > n_dorsalis_scapulae
	ursprung > processus_spinosi_hwk_1_4
	ansatz > margo_medialis_scapula
	funktion {"typ":"fixation"} > scapula
	funktion {"typ":"zieht_nach_kranial_medial"} > scapula