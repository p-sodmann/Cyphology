	teil_des > humerus
ursprung medialer_humerus {"name":"medialer Humerus"}
	teil_des > humerus
ursprung processus_corcoideus_scapulae {"name":"Processus corcoideus scapulae"}
ursprung processus_spinosi_hwk_1_4 {"name":"processus spinosi HWK 1-4"}
ursprung processus_spinosi_hwk_6_7 {"name":"processus spinosi HWK 6-7"}
ursprung tuberculum_infraglenoidale_scapulae {"name":"Tuberculum infraglenoidale scapulae"}
ursprung tuberculum_supraglenoidale_scapulae {"name":"Tuberculum supraglenoidale scapulae"}

#Flexoren Oberarmmuskulatur

muskelgruppe flexoren {"name":"Flexoren"}
	is_a > obere_extremitäten

muskel m_biceps_brachii {"name":"Musculus biceps brachii"}
	innervation > n_musculocutaneaus
	funktion {"typ":"flexion"} > articulatio_cubiti
	funktion {"typ":"supination"} > articulatio_cubiti
	funktion {"typ":"anteversion"} > articulatio_humeri
	funktion {"typ":"innenrotation"} > articulatio_humeri
	funktion {"typ":"abduktion"} > articulatio_humeri
	ansatz > tuberositas_radii

muskelteil caput_longum_m_biceps_brachii {"name":"Caput longum des Musculus biceps brachii"}
	is_a > m_biceps_brachii
	ursprung > tuberculum_supraglenoidale_scapulae

muskelteil caput_breve_m_biceps_brachii {"name":"Caput breve des Musculus biceps brachii"}
	is_a > m_biceps_brachii
	ursprung > processus_corcoideus_scapulae

muskel m_brachilis {"name":"Musculus brachilis"}
	is_a > flexoren
	innervation > n_musculocutaneaus
	funktion {"typ":"flexion"} > articulatio_cubiti
	ansatz > ventrale_ulna

muskel m_coracobrachialis {"name":"Musculus coracobrachialis"}
	is_a > flexoren
	innervation > n_musculocutaneaus
	funktion {"typ":"adduktion"} > brachium
	funktion {"typ":"innenrotation"} > brachium

#Extensoren Oberarmmuskulatur

muskelgruppe extensoren {"name":"extensoren"}
	is_a > obere_extremitäten

muskel m_triceps_brachii {"name":"Musculus tricpes brachii"}
	is_a > extensoren
	innervation > n_radialis
	funktion {"typ":"extension"} > articulatio_cubiti
	funktion {"typ":"adduktion"} > articulatio_humeri
	funktion {"typ":"retoversion"} > articulatio_humeri
	funktion {"typ":"innenrotation"} > articulatio_humeri
	ansatz > olecranon

muskelteil caput_longum_m_triceps_brachii {"name":"Caput longum des Musculus tricpes brachii"}
	is_a > m_triceps_brachii
	ursprung > tuberculum_infraglenoidale_scapulae

muskelteil caput_mediale_m_triceps_brachii {"name":"Caput mediale des Musculus tricpes brachii"}
	is_a > m_triceps_brachii
	ursprung > medialer_humerus

muskelteil caput_laterale_m_triceps_brachii {"name":"Caput laterale des Musculus tricpes brachii"}
	is_a > m_triceps_brachii
	ursprung > lateraler_humerus

#Schultermuskulatur

muskelgruppe schultermuskulatur {"name":"schultermuskulatur"}
	is_a > obere_extremitäten

muskel m_serratus_anterior {"name":"Musculus serratus anterior"}
	is_a > schultermuskulatur
	innervation > n_thoracicus
	funktion {"typ":"drehung_lateral"} > scapula
	funktion {"typ":"fixation"} > scapula
	ursprung > 1_9_rippe
	ansatz > margo_medialis_scapula

muskel m_trapezius {"name":"Musculus trapezius"}
	is_a > schultermuskulatur
	innervation > n_accessorius
	ursprung > hinterhaupt
	ursprung > dornenfortsätze_hws
	ursprung > dornenfortsätze_bws
