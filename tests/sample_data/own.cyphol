	is_a > extremitäten
anatomie oberarmmuskulatur {"name":"Oberarmmuskulatur"}
	is_a > obere_extremitäten

#Ursprung
ursprung 1_9_rippe {"name":"1-9 Rippe"}
ursprung dornenfortsätze_bws {"name":"Dornenfortsätze BWS"}
ursprung dornenfortsätze_hws {"name":"Dornenfortsätze HWS"}
